{
	"name": "@codeeditorland/plugins",
	"lint-staged": {
		"!(**/tests/**/*)*.js": [
			"prettier --write"
		],
		"!(**/tests/**/*)*.jsx": [
			"prettier --write"
		],
		"!(**/tests/**/*)*.ts": [
			"prettier --write"
		],
		"!(**/tests/**/*)*.tsx": [
			"prettier --write"
		],
		"*.rs": [
			"cargo fmt --"
		],
		"*.toml": [
			"taplo format"
		]
	},
	"dependencies": {},
	"devDependencies": {
		"@changesets/cli": "2.27.11",
		"@taplo/cli": "0.7.0",
		"lint-staged": "15.3.0"
	}
}
